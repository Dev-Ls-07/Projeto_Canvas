        "%pip -q install google-genai\n",
        "\n",
        "import os\n",
        "from google.colab import userdata\n",
        "os.environ[\"GOOGLE_API_KEY\"] = userdata.get('GOOGLE_API_KEY')\n",
        "\n",
        "import google.generativeai as genai\n",
        "\n",
        "genai.configure(api_key=os.environ[\"GOOGLE_API_KEY\"])\n",
        "\n",
        "# Defina uma função para gerar conteúdo com base no Modelo de Negócios Canvas\n",
        "def generate_canvas_section(section_name, prompt):\n",
        "    \"\"\"Generates content for a specific section of the Business Model Canvas.\"\"\"\n",
        "    model = genai.GenerativeModel('gemini-1.5-flash') # Use um modelo adequado\n",
        "    response = model.generate_content(f\"Como empreendedor, me ajude a preencher a seção de '{section_name}' do meu Business Model Canvas. Aqui está o meu foco: {prompt}\")\n",
        "    return response.text\n",
        "\n",
        "# Função para orientar o usuário no preenchimento do Business Model Canvas\n",
        "def build_business_plan():\n",
        "    print(\"Olá! Vamos construir seu plano de negócios usando o Business Model Canvas.\")\n",
        "    print(\"Vou te guiar por cada uma das nove seções.\")\n",
        "\n",
        "    canvas_data = {}\n",
        "\n",
        "    # Principais parceiros\n",
        "    prompt_partners = input(\"Quem são seus principais parceiros e fornecedores? \")\n",
        "    canvas_data['Parceiros-Chave'] = generate_canvas_section(\"Parceiros-Chave\", prompt_partners)\n",
        "    print(\"\\nConteúdo gerado para Parceiros-Chave:\")\n",
        "    print(canvas_data['Parceiros-Chave'])\n",
        "\n",
        "    # Principais atividades\n",
        "    prompt_activities = input(\"\\nQuais são as atividades mais importantes que sua empresa deve realizar? \")\n",
        "    canvas_data['Atividades-Chave'] = generate_canvas_section(\"Atividades-Chave\", prompt_activities)\n",
        "    print(\"\\nConteúdo gerado para Atividades-Chave:\")\n",
        "    print(canvas_data['Atividades-Chave'])\n",
        "\n",
        "    # Recursos principais\n",
        "    prompt_resources = input(\"\\nQuais são os recursos essenciais (físicos, intelectuais, humanos, financeiros) que sua proposta de valor exige? \")\n",
        "    canvas_data['Recursos-Chave'] = generate_canvas_section(\"Recursos-Chave\", prompt_resources)\n",
        "    print(\"\\nConteúdo gerado para Recursos-Chave:\")\n",
        "    print(canvas_data['Recursos-Chave'])\n",
        "\n",
        "    # Propostas de valor\n",
        "    prompt_value = input(\"\\nQual problema você está resolvendo para seus clientes? Que valor você entrega a eles? \")\n",
        "    canvas_data['Proposições-de-Valor'] = generate_canvas_section(\"Proposições-de-Valor\", prompt_value)\n",
        "    print(\"\\nConteúdo gerado para Proposições-de-Valor:\")\n",
        "    print(canvas_data['Proposições-de-Valor'])\n",
        "\n",
        "    # Relacionamento com o cliente\n",
        "    prompt_relationships = input(\"\\nQue tipo de relacionamento você estabelecerá com seus clientes? Como você os atrairá, reterá e expandirá? \")\n",
        "    canvas_data['Relacionamento-com-Clientes'] = generate_canvas_section(\"Relacionamento-com-Clientes\", prompt_relationships)\n",
        "    print(\"\\nConteúdo gerado para Relacionamento com Clientes:\")\n",
        "    print(canvas_data['Relacionamento-com-Clientes'])\n",
        "\n",
        "    # Canais\n",
        "    prompt_channels = input(\"\\nComo você alcançará seus segmentos de clientes e entregará sua proposta de valor? \")\n",
        "    canvas_data['Canais'] = generate_canvas_section(\"Canais\", prompt_channels)\n",
        "    print(\"\\nConteúdo gerado para Canais:\")\n",
        "    print(canvas_data['Canais'])\n",
        "\n",
        "    # Segmentos de clientes\n",
        "    prompt_segments = input(\"\\nPara quem você está criando valor? Quem são seus clientes mais importantes? \")\n",
        "    canvas_data['Segmentos-de-Clientes'] = generate_canvas_section(\"Segmentos-de-Clientes\", prompt_segments)\n",
        "    print(\"\\nConteúdo gerado para Segmentos de Clientes:\")\n",
        "    print(canvas_data['Segmentos-de-Clientes'])\n",
        "\n",
        "    # Estrutura de custos\n",
        "    prompt_costs = input(\"\\nQuais são os custos mais importantes inerentes ao seu modelo de negócios? \")\n",
        "    canvas_data['Estrutura-de-Custos'] = generate_canvas_section(\"Estrutura-de-Custos\", prompt_costs)\n",
        "    print(\"\\nConteúdo gerado para Estrutura de Custos:\")\n",
        "    print(canvas_data['Estrutura-de-Custos'])\n",
        "\n",
        "    # Fluxos de receita\n",
        "    prompt_revenue = input(\"\\nComo sua empresa gerará receita? De onde virá o dinheiro? \")\n",
        "    canvas_data['Fontes-de-Receita'] = generate_canvas_section(\"Fontes-de-Receita\", prompt_revenue)\n",
        "    print(\"\\nConteúdo gerado para Fontes de Receita:\")\n",
        "    print(canvas_data['Fontes-de-Receita'])\n",
        "\n",
        "    print(\"\\nParabéns! Você preencheu as seções do seu Business Model Canvas.\")\n",
        "    print(\"\\nAqui está um resumo do seu plano de negócios:\")\n",
        "    for section, content in canvas_data.items():\n",
        "        print(f\"\\n--- {section} ---\")\n",
        "        print(content)\n",
        "\n",
        "# Execute o construtor de plano de negócios\n",
        "if __name__ == \"__main__\":\n",
        "    build_business_plan()\n"
      ]
    }
  ]
}